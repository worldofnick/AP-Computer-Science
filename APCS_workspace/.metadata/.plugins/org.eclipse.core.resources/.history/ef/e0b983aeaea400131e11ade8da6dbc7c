
public class StackMain {

	public static void main(String[] args) {
		Stack stack = new Stack();
		String equation = "5 1 2 + 4 * + 3 -";
		String[] splitEquation = equation.split(" ");
		
		// Fill Stack
		for(String s : splitEquation) {
			stack.push(s);
		}

		// Math
		//String result = postfix(stack);
		//System.out.println(result);
		
	}
	
	private static String postfix(Stack stack) {
		Stack output = new Stack();
		while (!stack.isEmpty()) {
			if (isNumeric(stack.peek())) {
				output.push(stack.peek());
			} else if(stack.peek().matches("[*-/+]")) {
				String operator = stack.pop();
				
				switch (operator) {
				case "+":
					
					break;
					
				case "-":
					
					break;
					
				case "/":
					
					break;
					
				case "*":
				
					break;

				default:
					break;
				}
			}
		}
		
		return null;
	}
	
	public static boolean isNumeric(String str)  
	{  
	  try  
	  {  
	    double d = Double.parseDouble(str);  
	  }  
	  catch(NumberFormatException nfe)  
	  {  
	    return false;  
	  }  
	  return true;  
	}


}
